{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": 0,
        "no-console": 0,
        "quotes": 0,
        "no-underscore-dangle": 0,
        "arrow-parens": 0,
        "no-param-reassign": 0,
        "no-restricted-globals": 0,
        "no-return-assign": 0,
        "no-mixed-operators": 0,
        "no-unused-vars": 0,
        "no-undef": 0,
        "quote-props": 0,
        "max-len": 0,
        "arrow-body-style": 0,
        "no-confusing-arrow": 0
    }
}